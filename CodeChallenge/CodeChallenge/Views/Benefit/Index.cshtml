@model IEnumerable<CodeChallenge.Models.Benefit>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Coverage)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CostofBenefit)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiscountLetter)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Discount)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Coverage)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CostofBenefit)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiscountLetter)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Discount)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.BenefitID }) |
            @Html.ActionLink("Details", "Details", new { id=item.BenefitID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.BenefitID })
        </td>
    </tr>
}

</table>
